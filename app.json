{
  "connections": [
    {
      "port": 8080,
      "routes": {
        "timeout": {
          "server": 10000
        }
      },
      "load": {
        "maxHeapUsedBytes": 1073741824,
        "maxRssBytes": 2147483648,
        "maxEventLoopDelay": 5000
      },
      "labels": [
        "api",
        "http"
      ]
    }
  ],
  "server": {
    "load": {
      "sampleInterval": 1000
    }
  },
  "registrations": [
    {
      "plugin": {
        "register": "good",
        "options": {
          "reporters": {
            "console": [
              {
                "module": "good-squeeze",
                "name": "Squeeze",
                "args": [
                  {
                    "log": "*",
                    "response": "*",
                    "request": "*",
                    "ops": "*",
                    "error": "*"
                  }
                ]
              },
              {
                "module": "good-squeeze",
                "name": "SafeJson"
              },
              "stdout"
            ]
          }
        }
      }
    },
    {
      "plugin": "./lib/"
    }
  ]
}
